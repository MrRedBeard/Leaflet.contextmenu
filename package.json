{
  "name": "leaflet-contextmenu",
  "version": "1.6.1",
  "description": "A context menu for Leaflet",
  "scripts": {
    "build": "npm run build:iife && npm run build:esm && npm run build:umd",
    "build:iife": "rollup -c rollup.config.iife.js",
    "build:esm": "rollup -c rollup.config.esm.js",
    "build:umd": "rollup -c rollup.config.umd.js"
  },
  "type": "module",
  "main": "./dist/leaflet.contextmenu.umd.js",
  "module": "./dist/leaflet.contextmenu.esm.js",
  "style": "./dist/leaflet.contextmenu.css",
  "exports": {
    "import": "./dist/leaflet.contextmenu.esm.js",
    "require": "./dist/leaflet.contextmenu.umd.js",
    "./dist/leaflet.contextmenu.css": "./dist/leaflet.contextmenu.css"
  },
  "directories": {
    "dist": "dist",
    "images": "images",
    "src": "src"
  },
  "file": "dist/leaflet.contextmenu.iife.js",
  "files": [
    "dist/",
    "dist/leaflet.contextmenu.css",
    "leaflet.contextmenu.js",
    "index.html"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/MrRedBeard/Leaflet.contextmenu.git"
  },
  "keywords": [
    "leaflet",
    "contextmenu",
    "context",
    "menu",
    "submenu"
  ],
  "author": "MrRedBeard and Adam Ratcliffe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrRedBeard/Leaflet.contextmenu/issues"
  },
  "homepage": "https://github.com/MrRedBeard/Leaflet.contextmenu/issues",
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "postcss-banner": "^4.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "vite": "^7.0.4"
  }
}
